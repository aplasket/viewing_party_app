<div class="users">
  <%= button_to "Discover Movies", discover_path, method: :get %>

<% if current_user %>
  <%= button_to "Dashboard", dashboard_path, method: :get %>

  <h3>Existing Users</h3>
  <% @users.each do |user| %>
    <ul>
    <li><%= user.email %></li>
    </ul>
  <% end %>
<% end %>
</div>
